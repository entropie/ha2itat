.galleries-preview
  .galleries-head= nlink(path(:backend_galleries_show, slug: gallery.ident), gallery.ident)
  .backend-list-content
    .backend-galleries-list
      %ul.galleries-list
        - gallery.images.each do |img|
          %li
            .img-box.clipboard-click{style: img.css_background_defintion, "data-clipboard-content": '#{IMG("%s", "%s")}' % [gallery.ident, img.ident]}
              %span= img.ident
